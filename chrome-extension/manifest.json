{
  "manifest_version": 3,
  "name": "StatBase",
  "description": "Instagram profilinde grid üzerindeki son 12 gönderiden (pinli hariç) beğeni/yorum ve Ortalama ER hesaplar.",
  "version": "0.1.4",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.instagram.com/*"],
      "js": ["content.js"],
      "css": ["panel.css"],
      "run_at": "document_idle"
    }
  ],
  "host_permissions": ["https://www.instagram.com/*"],
  "permissions": []
}
